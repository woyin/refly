NODE_ENV=development

# Sandbox worker HTTP port
PORT=7651
LOG_LEVEL=info

# Redis (same network as docker compose)
REDIS_HOST=refly_redis
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# Queue limits
QUEUE_MAX_CONCURRENCY=30
QUEUE_MAX_QUEUE_SIZE=500

# Provider: Scalebox (optional)
PROVIDERS_SCALEBOX_API_KEY=
PROVIDERS_SCALEBOX_TEMPLATE_NAME=slim-v4
PROVIDERS_SCALEBOX_MAX_SANDBOXES=10
PROVIDERS_SCALEBOX_AUTO_PAUSE_DELAY_MS=120000
PROVIDERS_SCALEBOX_SANDBOX_TIMEOUT_MS=3600000

# Provider: E2B (optional)
PROVIDERS_E2B_API_KEY=
PROVIDERS_E2B_TEMPLATE_NAME=slim-v4
PROVIDERS_E2B_MAX_SANDBOXES=10
PROVIDERS_E2B_AUTO_PAUSE_DELAY_MS=120000
PROVIDERS_E2B_SANDBOX_TIMEOUT_MS=3600000
